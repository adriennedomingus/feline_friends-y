<% if current_user %>
  <li> <%= link_to "Logged in as #{current_user.username}", dashboard_path(current_user) %></li>
  <li><%= link_to "Logout", logout_path, method: "delete"%></li>
<% else %>
  <li><a href="<%= login_path %>">Login</a></li>
  <li><a href="<%= new_user_path %>">Create Account</a></li>
<% end %>
